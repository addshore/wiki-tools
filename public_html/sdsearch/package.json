{
  "private": true,
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:css": "npm run build:sass",
    "build:js": "webpack -d",
    "build:sass": "node-sass scss/style.scss style.css",
    "dist": "npm run update-locales && npm run build:css && webpack -p",
    "postinstall": "npm run build",
    "update-locales": "node etc/update-locales.js",
    "watch": "chokidar '../common/*.scss' 'scss/*.scss' -c 'npm run build:css' & webpack -dw"
  },
  "dependencies": {
    "donot": "^0.2.0",
    "regenerator-runtime": "^0.13.5",
    "valenski": "^1.4.0",
    "vue": "^2.6.11",
    "vue-i18n": "^8.17.7",
    "vue-simple-suggest": "^1.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chokidar-cli": "^2.1.0",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "http-server": "^0.12.1",
    "node-fetch": "^2.6.7",
    "node-sass": "^4.14.0",
    "vue-loader": "^15.9.1",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.4.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
